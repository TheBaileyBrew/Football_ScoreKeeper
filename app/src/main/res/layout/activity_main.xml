<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/football_field"
    android:orientation="vertical"
    tools:context="com.thebaileybrew.scorekeeper.football.footballscorekeeper.MainActivity">

    <!--TEAM ONE AND TWO SPINNERS ** HORIZONTAL-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Spinner
            android:id="@+id/teamOne_spinner"
            android:layout_margin="@dimen/small_med_padding"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="center"
            />
        <Spinner
            android:id="@+id/teamTwo_spinner"
            android:layout_margin="@dimen/small_med_padding"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="center"
            />
    </LinearLayout>

    <!--TEAM LOGOS AND SCORE ** HORIZONTAL-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/team_one_spinner_team_logo"
            android:gravity="right"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="70dp"
            />
        <TextView
            android:id="@+id/team_one_score"
            android:gravity="center"
            android:layout_margin="@dimen/small_padding"
            android:paddingRight="@dimen/med_medium_padding"
            android:paddingLeft="@dimen/med_medium_padding"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:textSize="@dimen/xtra_large_font"
            android:text="0"
            />
        <ImageView
            android:id="@+id/team_two_spinner_team_logo"
            android:gravity="right"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="70dp"
            />
        <TextView
            android:id="@+id/team_two_score"
            android:gravity="center"
            android:layout_margin="@dimen/small_padding"
            android:paddingRight="@dimen/med_medium_padding"
            android:paddingLeft="@dimen/med_medium_padding"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:textSize="@dimen/xtra_large_font"
            android:text="0"
            />
    </LinearLayout>

    <!--TEAM STARTING QUARTERBACKS AND STAT REGISTER-->
    <LinearLayout
        android:id="@+id/qb_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/small_padding"
        android:layout_marginRight="@dimen/small_padding"
        android:background="#66999999">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_med_padding"
            android:layout_gravity="center_vertical"
            android:text="Starting Quarterback:"
            android:textSize="@dimen/small_med_font"
            android:textStyle="bold"
            android:textColor="#FFFFFF"/>
        <TextView
            android:id="@+id/team_one_passing_player_name"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/small_padding"
            android:layout_gravity="center_vertical"
            android:text="Player name"
            android:textColor="#000000"
            android:textSize="@dimen/small_med_font"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_med_padding"
            android:layout_gravity="center_vertical"
            android:text="Starting Quarterback:"
            android:textSize="@dimen/small_med_font"
            android:textStyle="bold"
            android:textColor="#FFFFFF"/>
        <TextView
            android:id="@+id/team_two_passing_player_name"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="Player name"
            android:textColor="#000000"
            android:textSize="@dimen/small_med_font"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/small_padding"
        android:layout_marginRight="@dimen/small_padding"
        android:layout_marginBottom="@dimen/small_padding"
        android:background="#66999999">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="Attempts: "
            android:textSize="@dimen/small_font"/>
        <TextView
            android:id="@+id/team_one_qb_attempts_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="Completions: "
            android:textSize="@dimen/small_font"/>
        <TextView
            android:id="@+id/team_one_qb_comp_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="Interceptions:"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:id="@+id/team_one_intercept_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="Attempts: "
            android:textSize="@dimen/small_font"/>
        <TextView
            android:id="@+id/team_two_qb_attempts_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="Completions: "
            android:textSize="@dimen/small_font"/>
        <TextView
            android:id="@+id/team_two_qb_comp_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="Interceptions:"
            android:textSize="@dimen/small_font"/>
        <TextView
            android:id="@+id/team_two_intercept_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="0"
            android:textSize="@dimen/small_font"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_med_padding"
            android:text="Team Stats"
            android:textSize="@dimen/small_med_font"
            android:textStyle="bold"
            android:textColor="#FFFFFF"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_med_padding"
            android:text="Team Stats"
            android:textSize="@dimen/small_med_font"
            android:textStyle="bold"
            android:textColor="#FFFFFF"/>
    </LinearLayout>
    <!--TEAM ONE AND TWO YARDAGE HEADER LINES ** HORIZONTAL-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/small_padding"
        android:layout_marginRight="@dimen/small_padding"
        android:background="#66999999">
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_one_passing"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="@string/fumble"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_one_fumble_count"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="0"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_one_recieving"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="@string/rec"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_one_rec_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="0"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_one_rushing"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="@string/rush"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_one_rush_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="0"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_two_passing"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="@string/fumble"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_two_fumble_count"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="0"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_two_recieving"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="@string/rec"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_two_rec_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="0"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_two_rushing"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="@string/rush"
            />
        <TextView
            android:layout_gravity="center_horizontal"
            android:id="@+id/team_two_rush_score"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="0"
            />
    </LinearLayout>

    <!--TEAM ONE AND TWO YARDAGE TOTALS ** HORIZONTAL-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="@dimen/small_med_padding">
    </LinearLayout>

    <!--TEAM ONE AND TWO OFFENSE / DEFENSE SLIDERS ** HORIZONTAL-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </LinearLayout>

    <!--TEAM ONE AND TWO SCORING BUTTONS ** HORIZONTAL-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ToggleButton
            android:id="@+id/team_one_toggle"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/small_med_padding"
            android:layout_height="match_parent"
            android:background="@drawable/toggle_switch"
            android:textOn=""
            android:textOff=""
            />
        <!--TEAM ONE BUTTONS ** VERTICAL-->
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="125dp"
            android:orientation="vertical">
            <Button
                android:id="@+id/team_one_off_td"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="TOUCHDOWN"
                android:onClick="touchdownClickTeamOne"/>
            <Button
                android:id="@+id/team_one_off_extra"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="EXTRA POINT"
                android:onClick="extraPointClickTeamOne"/>
            <Button
                android:id="@+id/team_one_off_plustwo"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="TWO POINT CONVERSION"
                android:onClick="conversionClickTeamOne"/>
            <Button
                android:id="@+id/team_one_off_fg"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="FIELDGOAL"
                android:onClick="fieldgoalClickTeamOne"/>

        </LinearLayout>
        <ToggleButton
            android:id="@+id/team_two_toggle"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/small_med_padding"
            android:layout_height="match_parent"
            android:background="@drawable/toggle_switch"
            android:checked="true"
            android:textOn=""
            android:textOff=""
            />
        <!--TEAM TWO BUTTONS ** VERTICAL-->
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="125dp"
            android:orientation="vertical">
            <Button
                android:id="@+id/team_two_off_td"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="TOUCHDOWN"
                android:onClick="touchdownClickTeamTwo"/>
            <Button
                android:id="@+id/team_two_off_extra"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="EXTRA POINT"
                android:onClick="extraPointClickTeamTwo"/>
            <Button
                android:id="@+id/team_two_off_plustwo"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="TWO POINT CONVERSION"
                android:onClick="conversionClickTeamTwo"/>
            <Button
                android:id="@+id/team_two_off_fg"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:textSize="@dimen/small_med_font"
                android:text="SAFETY"
                android:onClick="fieldgoalClickTeamTwo"/>
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_margin="@dimen/small_med_padding"
        android:background="#FFFFFF">
    </View>
    <!--BUTTONS FOR PASSING STATISTICS-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="@string/pass_yd"
        android:textSize="@dimen/med_large_font"
        android:textColor="#000000"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">
        <Button
            android:id="@+id/team_one_qb_complete"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="completion"/>
        <Button
            android:id="@+id/team_one_qb_incomplete"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="incomplete"/>

        <Button
            android:id="@+id/team_two_qb_complete"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="completion"/>
        <Button
            android:id="@+id/team_two_qb_incomplete"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="incomplete"/>
    </LinearLayout>
    <!--BUTTONS FOR FUMBLE AND YARDAGE ADDITION-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">
        <Button
            android:id="@+id/team_one_qb_fumble"
            android:onClick="fumbleClickTeamOne"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="fumble"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"/>
        <Button
            android:id="@+id/team_one_qb_interception"
            android:onClick="interceptionClickTeamOne"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="interception"/>
        <Button
            android:id="@+id/team_two_qb_fumble"
            android:onClick="fumbleClickTeamTwo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="fumble"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"/>
        <Button
            android:id="@+id/team_two_qb_interception"
            android:onClick="interceptionClickTeamTwo"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="interception"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_margin="@dimen/small_med_padding"
        android:background="#FFFFFF">
    </View>

    <!--BUTTONS FOR RUSHING STATISTICS-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="@string/rush_yd"
        android:textSize="@dimen/med_large_font"
        android:textColor="#000000"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">
        <Button
            android:id="@+id/team_one_rb_attempt"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="rush attempt"/>
        <Button
            android:id="@+id/team_one_rb_fumble"
            android:onClick="fumbleClickTeamOne"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="fumble"/>
        <Button
            android:id="@+id/team_two_rb_attempt"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="rush attempt"/>

        <Button
            android:id="@+id/team_two_rb_fumble"
            android:onClick="fumbleClickTeamTwo"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="fumble"/>
    </LinearLayout>
    <!--BUTTONS FOR FUMBLE AND YARDAGE ADDITION-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <Button
            android:id="@+id/team_one_rb_minus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="-1"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"
            android:onClick="rushingYardsTeamOneMinus"/>
        <Button
            android:id="@+id/team_one_rb_plus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="+1"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"
            android:onClick="rushingYardsTeamOnePlus"/>
        <Button
            android:id="@+id/team_one_rb_plus_ten"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="+10"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"
            android:onClick="rushingYardsTeamOneTenPlus"/>

        <Button
            android:id="@+id/team_two_rb_minus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="-1"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"
            android:onClick="rushingYardsTeamTwoMinus"/>
        <Button
            android:id="@+id/team_two_rb_plus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="+1"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"
            android:onClick="rushingYardsTeamTwoPlus"/>
        <Button
            android:id="@+id/team_two_rb_plus_ten"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="+10"
            android:textSize="@dimen/small_font"
            android:layout_weight="1"
            android:onClick="rushingYardsTeamTwoTenPlus"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_margin="@dimen/small_med_padding"
        android:background="#FFFFFF">
    </View>

    <!--BUTTONS FOR RECEIVING STATISTICS-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="@string/rec_yd"
        android:textSize="@dimen/med_large_font"
        android:textColor="#000000"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">
        <Button
            android:id="@+id/team_one_wr_attempt"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="target"/>
        <Button
            android:id="@+id/team_one_wr_fumble"
            android:onClick="fumbleClickTeamOne"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="fumble"/>
        <Button
            android:id="@+id/team_two_wr_attempt"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="target"/>

        <Button
            android:id="@+id/team_two_wr_fumble"
            android:onClick="fumbleClickTeamTwo"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_font"
            android:text="fumble"/>
    </LinearLayout>
    <!--BUTTONS FOR FUMBLE AND YARDAGE ADDITION-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <Button
